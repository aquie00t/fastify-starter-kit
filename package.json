{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "Fastify API Starter Kit",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:check:fix": "prettier --check . --write",
    "prepare": "husky",
    "test": "tap"
  },
  "keywords": [
    "fastify-api",
    "fastify-starter-kit",
    "fastify",
    "api",
    "javascript",
    "node"
  ],
  "author": "Aquie",
  "license": "MIT",
  "type": "module",
  "lint-staged": {
    "*.js": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/mongodb": "^9.0.2",
    "@fastify/sensible": "^6.0.3",
    "fastify": "^5.3.2",
    "fastify-plugin": "^5.0.1",
    "fluent-json-schema": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "3.5.3",
    "tap": "^21.1.0"
  }
}
